{
  "name": "@volta-cli/action",
  "version": "1.7.0",
  "private": true,
  "description": "Setup volta for usage in your CI runs",
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volta-cli/action.git"
  },
  "license": "MIT",
  "author": "Robert Jackson <me@rwjblue.com>",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm-run-all build:clean build:ts build:dist",
    "build:clean": "rimraf dist lib",
    "build:dist": "ncc build src/index.ts",
    "build:ts": "tsc",
    "lint": "npm-run-all lint:*",
    "lint:files": "eslint --cache .",
    "lint:types": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@actions/github": "^5.0.0"
  },
  "devDependencies": {
    "@actions/core": "^1.5.0",
    "@actions/exec": "^1.1.0",
    "@actions/glob": "^0.2.0",
    "@actions/io": "^1.1.1",
    "@actions/tool-cache": "^1.7.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.14.2",
    "@types/semver": "^7.3.8",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vercel/ncc": "^0.30.0",
    "broccoli-test-helper": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "find-up": "^6.0.0",
    "got": "^11.8.0",
    "jest": "^27.1.0",
    "jest-circus": "^27.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "release-it": "^14.2.1",
    "release-it-lerna-changelog": "^3.1.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2",
    "uuid": "^8.3.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "hooks": {
      "after:bump": "npm run build"
    },
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": {
      "publish": false
    }
  }
}
